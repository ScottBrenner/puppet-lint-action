{
  "problemMatcher": [
    {
      "owner": "puppet-lint",
      "pattern": [
        {
          "regexp": "^(.*?):(\\d+):(\\d+): (warning|error): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}